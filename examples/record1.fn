person = object(firstname lastname mail)

run = fn () {
    Print = fn (X) { io.format("~p~n" [X]) }
    P = person("mariano" "guerra" "mail")
    R = P(to record)

    R1 = person.R[firstname="Mariano" lastname="Guerra"]
    Print(person.R.firstname)
    Print(R1)

    R2 = person[firstname="Bob" lastname="Esponja" mail="bob@esponja.com"]
    Print(R2)
}
