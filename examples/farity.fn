add_one = fn (Number) {
    Number + 1
}

run = fn () {
    # a way to pass a reference to a function withouth
    # creating a lambda
    Result = lists.map(fn add_one/1, lists.seq(1 10))
    io.format("~p~n", [Result])
    Result1 = lists.map(fn farity.add_one/1, lists.seq(1 10))
    io.format("~p~n", [Result1])
}
