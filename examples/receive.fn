public run = fn () {
	io.format("testing receive: ")
    receive (Something) when is_integer(Something) {
    	Something + 1
    } after 100 {
	    io.format(".")
    }

    receive _SomethingElse {
    	ok
    } after 100 {
	    io.format(".")
    }

    receive "some string" {
    	ok
    } (5) {
        five
    } true {
        true
    } after 100 {
	    io.format(".")
    }

	io.format("~n")
}
