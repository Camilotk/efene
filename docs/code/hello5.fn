
@public
hello = fn (Name) {
    if Name == "Winston Churchil" {
        io.format("Well hello sir~n")
    }
    else {
        io.format("Hello ~s!~n", [Name])
    }
}

@public
hello = fn () {
    hello("World")
}

@public
run = fn () {
    hello()
    hello("coders")
    hello("Winston Churchil")
}
